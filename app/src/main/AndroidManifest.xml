<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinemedicine">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineMedicine"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.MedicineActivity"></activity>
        <activity android:name=".Activity.ForgotPassword" />
        <activity android:name=".Activity.PaymentOptions" />
        <activity android:name=".Activity.OrderDetail" />
        <activity android:name=".Activity.OtpActivity" />
        <activity android:name=".SignUp.SignUpActivity" />
        <activity android:name=".Login.Login" />
        <activity android:name=".Activity.PayActivity" />
        <activity android:name=".Activity.CheakOut" />
        <activity android:name=".Activity.OrderActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.Doctor_Dashbord" />
        <activity android:name=".Activity.Doctor_Login" />
        <activity android:name=".SplashScreen.Doctor_Splash" />
        <activity android:name=".SplashScreen.Splash" />
        <activity android:name=".Activity.Dashboard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>